.container {
  width: 100%;
  height: 110%;
  text-align: center;
  margin-top: 0px;
  padding-top: 0px;
  position: fixed;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content: center;
}

.register_org_name {
  color: #fff;
  font-size: 14px;
  padding: .5rem  4rem .5rem 1rem;
  background-color: #002c6b;
  border-radius: 4px;
}

.userinfo {
  display: flex;
  align-items: center;

}
.userinfo navigator{ width: auto;}
.userinfo-avatar {
  width: 64rpx;
  height: 64rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 20px;
}

navigator {
  width: 99%;
}

.list {
  width: 100%;
  height: 65%;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  margin-top: 20px;
}
.list image{
  width: 4rem;
  height: 4rem;
}
.container view {
  color: #fff;
}

.top_img {
  width: 100%;
  background-repeat: no-repeat;
  border: none;
  margin: 0 0 20px 0px;
  height: 25%;
}
.top_img image, .top_img > swiper , .top_img navigator{
  width: 100%;
  height: 100%;
}
.vall {
  border-radius: 6px;
  border: 0px solid #d2d1d1;
  align-items: center;
  justify-content: center;
  display: flex;
  width: 46%;
  background-repeat: no-repeat;
  background-size:100% 100%;
  -moz-background-size:100% 100%;
}

.v1 {
  height: 58%;
  margin: 1% 3% 1% 0%;
  background-color: #ef7d00;
  display: flex;
  flex-flow: column wrap;
  justify-content: center;
  align-items: center;
}
.v1 image{
  height: 6rem;
  width: 6rem;
}

.v23 {
  height: 58%;
  margin-top: 1%;
  width: 46%;
}

.v2 {
  width: 100%;
  height: 48%;
  margin: 0 0 4% 0;
  background-color: #80ba27;
  display:  flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;

}

.v3 {
  width: 100%;
  height: 49%;
  margin: 0 0 0 0;
  background-color: #00acda;
  
}

.v4 {
  height: 38%;
  margin: 2% 0% 0% 0%;
  padding-left: 0px;
  background-color: #2ab6b7;
}

.v5 {
  height: 38%;
  margin:  2% 0 0% 3%;
  background-color: #ea4b81;
}

.hp_img {
  border-radius: 60px;
}
